{
  "name": "davifsroberto",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:development": "ng serve --configuration=development",
    "start:homolog": "ng serve --configuration=homolog",
    "start:production": "ng serve --configuration=production",
    "build": "ng build",
    "build:production": "ng build --configuration=production",
    "build:homolog": "ng build --configuration=homolog",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:e2e": "npm run stop-server && ng serve & wait-on http://localhost:4210 && npx cypress run",
    "stop-server": "cross-env NG_FORCE_EXIT=true",
    "serve:ssr:davifsroberto": "node dist/davifsroberto/server/server.mjs",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "pretty:fix": "prettier --config ./.prettierrc --write \"./**/*.{ts,js,json,css,scss,html}\"",
    "pretty:check": "prettier --config ./.prettierrc --check \"./**/*.{ts,js,json,css,scss,html}\"",
    "prepare": "husky install"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/ssr": "^17.0.2",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.2",
    "@angular-eslint/builder": "17.1.0",
    "@angular-eslint/eslint-plugin": "17.1.0",
    "@angular-eslint/eslint-plugin-template": "17.1.0",
    "@angular-eslint/schematics": "17.1.0",
    "@angular-eslint/template-parser": "17.1.0",
    "@angular/cli": "^17.0.2",
    "@angular/compiler-cli": "^17.0.0",
    "@testing-library/angular": "^14.5.0",
    "@testing-library/cypress": "^10.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.10",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.4",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "typescript": "~5.2.2",
    "wait-on": "^7.2.0"
  }
}
